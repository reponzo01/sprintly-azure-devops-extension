{
    "manifestVersion": 1,
    "publisher": "EmanuelVargas",
    "description": "Azure Devops Extension to help facilitate releases at Foundation Software LLC.",
    "scopes": [
        "vso.code_manage",
        "vso.code_status",
        "vso.memberentitlementmanagement",
        "vso.graph",
        "vso.identity",
        "vso.tokens",
        "vso.project"
    ],
    "categories": [
        "Azure Repos"
    ],
    "branding": {
        "color": "rgb(34, 34, 34)",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/reponzo01/sprintly-azure-devops-extension.git"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}
